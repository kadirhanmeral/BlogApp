@model List<Post>


<div class="card mb-3">
    <div class="card-header">
        <h5 class="card-title mb-0">New Post</h5>
    </div>
    <div class="card-body">
        @foreach(var post in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0 fw-bold"><a href="/posts/details/@post.Url" style="color: #343a40;">@post.Title</a></h6>
            </div>
        }
       
    </div>
</div>